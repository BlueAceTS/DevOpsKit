    "Project",
    "Build",
    "Release",
    "ServiceConnection",
    "AgentPool"
  ],
  "AttestationExpiryPeriodInDays": {
    "Default": 90,
    "ControlSeverity": {
      "Critical": 7,
      "High": 30,
      "Medium": 60,
      "Low": 90
    }
  },
  "AllowAttestationByGroups": [
